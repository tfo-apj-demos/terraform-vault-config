replace: userAccountControl
userAccountControl: 66048
-

dn: CN=vault_dynamic,OU=VaultManagedAccounts,DC=hashicorp,DC=local
changetype: modify
add: member
member: CN={{.Username }},OU=VaultManagedAccounts,DC=hashicorp,DC=local 
